{
  "name": "aim-vue-cli-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode develop",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "build": "vite build --mode develop",
    "build:test": "vite build --mode test",
    "buid:prod": "vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "stylelint": "stylelint --cache --fix \"**/*.{html,vue,css,scss}\" --cache-location node_modules/.cache/stylelint/",
    "prepare": "git init && husky install",
    "pre-commit": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "element-plus": "^2.6.2",
    "pinia": "^2.1.7",
    "sass": "^1.72.0",
    "unplugin-icons": "^0.18.5",
    "vue": "^3.4.21",
    "vue-hooks-plus": "^1.8.9",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-hooks-plus/resolvers": "^1.2.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "stylelint": "^16.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "stylelint-prettier": "^5.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-csp": "^1.1.2",
    "vue-tsc": "^2.0.6"
  }
}
